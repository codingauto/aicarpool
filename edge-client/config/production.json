{
  "server": {
    "ssl": {
      "enabled": true
    }
  },
  "centralServer": {
    "timeout": 30000,
    "retryAttempts": 5,
    "retryDelay": 2000
  },
  "logging": {
    "level": "info",
    "maxSize": "50m",
    "maxFiles": "30d"
  },
  "monitoring": {
    "metricsInterval": 60000,
    "heartbeatInterval": 120000,
    "healthCheckInterval": 60000,
    "retentionDays": 30
  },
  "proxy": {
    "timeout": 180000,
    "retryAttempts": 5,
    "retryDelay": 2000,
    "rateLimit": {
      "windowMs": 60000,
      "max": 200
    }
  },
  "environment": "production",
  "debug": false
}