{
  "node": {
    "name": "edge-node-dev",
    "location": "Development",
    "endpoint": "http://localhost:8080"
  },
  "server": {
    "port": 8080,
    "ssl": {
      "enabled": false
    }
  },
  "centralServer": {
    "url": "http://localhost:3000",
    "wsUrl": "ws://localhost:3000/ws",
    "timeout": 10000
  },
  "logging": {
    "level": "debug"
  },
  "monitoring": {
    "metricsInterval": 10000,
    "heartbeatInterval": 30000,
    "healthCheckInterval": 15000
  },
  "environment": "development",
  "debug": true
}